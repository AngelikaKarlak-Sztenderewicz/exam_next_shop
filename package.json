{
  "name": "exam-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "docker-compose up -d && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "generate": "npx prisma generate",
    "seed": "npx prisma db seed",
    "tables": "docker exec -it exam-shop-db-1 psql -U postgres -d exam_shop",
    "migrations": "npx prisma migrate dev --name init && npx prisma db seed",
    "migrate": "prisma migrate deploy",
    "reset": "npx prisma migrate reset"
  },
  "dependencies": {
    "@prisma/client": "^6.19.1",
    "bcrypt": "^6.0.0",
    "next": "16.1.1",
    "next-auth": "^4.24.13",
    "prisma": "^6.19.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.70.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.23",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "prisma": "6.19.1",
    "tailwindcss": "^3.4.19",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
